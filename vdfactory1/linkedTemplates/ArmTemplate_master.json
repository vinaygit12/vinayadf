{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vdfactory1"
		},
		"vDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'vDataLakeStorage1'"
		},
		"vdb1LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'vdb1LS'"
		},
		"AzureBlobStorageLS1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "vstorage1"
		},
		"AzureKeyVaultLS1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://vkeyvault1.vault.azure.net/"
		},
		"upsert_linked_s1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "vdb2"
		},
		"vDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vdatalake1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vdfactory1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"vDataLakeStorage1_accountKey": {
						"value": "[parameters('vDataLakeStorage1_accountKey')]"
					},
					"vdb1LS_connectionString": {
						"value": "[parameters('vdb1LS_connectionString')]"
					},
					"AzureKeyVaultLS1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultLS1_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorageLS1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorageLS1_properties_typeProperties_connectionString_secretName')]"
					},
					"upsert_linked_s1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('upsert_linked_s1_properties_typeProperties_connectionString_secretName')]"
					},
					"vDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('vDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "vdfactory1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vdfactory1_ArmTemplate_0')]"
			]
		},
		{
			"name": "vdfactory1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vdfactory1_ArmTemplate_1')]"
			]
		}
	]
}